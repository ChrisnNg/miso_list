<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Miso list</title>

    <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
    <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />
    <link rel="stylesheet" href="/styles/layout.css" type="text/css" />
    <!--Nav CSS-->
    <link rel="stylesheet" href="/styles/nav.css" type="text/css" />
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/css/bootstrap.min.css" integrity="sha384-GJzZqFGwb1QTTN6wy59ffF1BuGJpLSa9DkKMp0DgiMDm4iYMj70gZWKYbI706tWS" crossorigin="anonymous">
    <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
    <script type="text/javascript" src="/scripts/app.js"></script>
    <!--Font Awesome-->
    <script src="https://kit.fontawesome.com/f07023f341.js"></script>
    <meta name='viewport' content='width=device-width, initial-scale=1.0, maximum-scale=1.0' />
  </head>

  <body>
    <nav class="gridLayered">


      <div class="">
        <div class="left2">
            <!-- <span class='left'>Miso List</span> -->
            <img class="left" class="img" src ="https://www.sccpre.cat/mypng/detail/172-1728754_japanese-akita-icon-wrapping-paper-shiba-inu.png" >
            <a class="iconLinks left" href="#bookList">
              <i class="fas fa-book fa-2x"></i>
            </a>
            <a class="iconLinks left" href="#movieList">
              <i class="fas fa-tv fa-2x"></i>
            </a>
            <a class="iconLinks left" href="#restList">
              <i class="fas fa-utensils fa-2x"></i>
            </a>
            <a class="iconLinks left" href="#productList">
              <i class="fas fa-shopping-cart fa-2x"></i>
            </a>
            <a class="iconLinks left" href="#miscList">
              <i class="fas fa-clipboard-list fa-2x"></i>
            </a>
        </div>
      </div>
    </nav>

    <header class="header">
      <div class="centerHeader">
          <h1 class="headerItem">Miso, search!</h1>
          <form action="/" method="POST" class="headerItem">
            <input type="text" name="searchEngine" style="width: 200px; margin: 1em">
            <button type="submit" class="btn btn-success">Add to List!</button>
          </form>
      </div>
    </header>
    <main>


  <% if (results) { %>
    <div id="productList">----------------------Products----------------------</div>
    <% let products = results['products']; %>
    <% for (let product of products) { %>
      <a href='/<%= product['name']%>'><%= product['name'] %></a><br>
    <% } %>
    <div id="bookList">----------------------Books----------------------</div>
    <% let books = results['books']; %>
    <% for (let book of books) { %>
      <p><%= book['name'] %> </p>
    <% } %>
    <div id="restList">----------------------Restaurants----------------------</div>
    <% let restaurants = results['restaurants']; %>
    <% for (let restaurant of restaurants) { %>
      <p><%= restaurant['name'] %> </p>
    <% } %>
    <div id="movieList">----------------------Movies----------------------</div>
    <% let movies = results['movies_and_series']; %>
    <% for (let movie of movies) { %>
      <p><%= movie['name'] %> </p>
    <% } %>
    <div id="miscList">----------------------Misc----------------------</div>
    <% let misc = results['misc']; %>
    <% for (let mis of misc) { %>
      <p><%= mis['name'] %> </p>
    <% } %>

  <% } %>
  </main>
  </body>
</html>
